					 (PWR_VISION	| PWR_ON)
					 (PWR_NPU_CORE	| PWR_OFF)
//					 (PWR_NPU_TOP	| PWR_ON)
					 (PWR_DLA	| PWR_ON)
					 (PWR_VENC	| PWR_ON) >;

