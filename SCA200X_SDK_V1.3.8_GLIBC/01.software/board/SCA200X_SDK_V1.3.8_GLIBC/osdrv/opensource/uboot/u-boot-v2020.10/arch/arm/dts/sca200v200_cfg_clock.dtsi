					(CGU_IFC_CLK			| CLK_ENABLE | IFC_FIX_PLL_CLK500		| 0)
					(CGU_EIS_CLK			| CLK_ENABLE | EIS_FIX_PLL_CLK500		| 0)
					(CGU_SCALER_CLK			| CLK_ENABLE | SCALER_FIX_PLL_CLK500		| 0)
