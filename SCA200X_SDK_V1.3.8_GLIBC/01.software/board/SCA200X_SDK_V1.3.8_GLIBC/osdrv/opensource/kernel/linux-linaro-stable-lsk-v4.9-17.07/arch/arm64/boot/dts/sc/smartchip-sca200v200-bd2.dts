/dts-v1/;
#include "smartchip-sca200v200.dtsi"
#include "smartchip-sca200v200-memory.dtsi"

/ {
	model = "SmartChip, Sca200v200 Development Board2";
};

&uart0 {
	status = "okay";
};

&uart1 {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&uart3 {
	status = "okay";
};

&uart4 {
	status = "okay";
};

&vbus_regulator0 {
	compatible = "regulator-fixed";
	regulator-name = "usb_otg0_vbus";
	regulator-min-microvolt = <5000000>;
	regulator-max-microvolt = <5000000>;
	gpio = <&portc0 3 GPIO_ACTIVE_HIGH>;
	enable-active-high;
};

&usb_otg0 {
	dr_mode = "host";
	vbus-supply = <&vbus_regulator0>;
	status = "okay";
};

&wdt0 {
	status = "disabled";
};

&rtc0 {
	status = "disabled";
};

&pwm0 {
	status = "disabled";
};

&pwm1 {
	status = "disabled";
};

&tzc{
	status = "disabled";
};

&spim0 {
	status = "disabled";
};

&spim1 {
	status = "disabled";
};

&spis0 {
	status = "disabled";
};

&spis1 {
	status = "disabled";
};

&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&i2c2 {
	status = "okay";
};

&i2c3 {
	status = "okay";
};

&i2c4 {
	status = "disabled";
};

&vqmmc_sdhci {
	gpios = <&portc0 25 GPIO_ACTIVE_HIGH>;
	gpios-states = <0>;
	states = <3300000 0
		  1800000 1>;
};

&emmc {
	/delete-property/voltage-ranges;
	/delete-property/no-1-8-v;
	vqmmc-supply = <&vqmmc_sdhci>;
	mmc-hs200-1_8v;
	status = "okay";
};

&vqmmc_sdmmc0 {
	gpios = <&portb0 21 GPIO_ACTIVE_HIGH>;
	gpios-states = <0>;
	states = <3300000 0
		  1800000 1>;
};

&sdmmc0 {
	vqmmc-supply = <&vqmmc_sdmmc0>;
	status = "okay";
};

&sdmmc1 {
	/delete-property/sd-uhs-sdr12;
	/delete-property/sd-uhs-sdr25;
	/delete-property/sd-uhs-sdr50;
	/delete-property/sd-uhs-sdr104;
//	status = "okay";
};

&mdio {
	status = "okay";

	phy0: ethernet-phy@1 {
		eee-broken-100tx;
		status = "okay";
	};
};

&gmac {
	status = "okay";
};

&qspi {
	status = "disabled";
};

&sec_uart {
	status = "okay";
};

&sec_timer {
	status = "okay";
};

&sec_wdt {
	status = "okay";
};
